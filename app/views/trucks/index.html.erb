<h1>All Trucks</h1>
<% @trucks.each do |truck| %>
  <a href="/trucks/<%= truck.id %>"><h3>NAME: <%= truck.name %></h3></a>
  <p>DESCRIPTION: <%= truck.description %></p>
  <p>ADDRESS: <%= truck.address %></p>
  <p>USER ID: <%= truck.user_id %></p>
<% end %>

<a href="/trucks/new">Add a new Truck</a>
